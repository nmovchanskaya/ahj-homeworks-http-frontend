{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
        ],
        "no-unused-vars": "off",
        "guard-for-in": "off",
        "no-underscore-dangle": "off",
        "no-console": "off",
        "no-plusplus": "off",
        "max-len": "off",
        "no-param-reassign": "off",
        "default-case": "off",
        "no-constructor-return": "off",
        "new-cap": "off",
        "no-loop-func": "off",
        "no-constant-condition": "off",
        "no-alert": "off",
        "no-lonely-if": "off",
        "class-methods-use-this": "off",
        "no-undef": "off",
        "no-empty-function": "off",
        "no-useless-constructor": "off"
    }
}
